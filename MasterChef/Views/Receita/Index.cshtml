@model IEnumerable<MasterChef.Models.Data.Receita>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>
            "ReceitaID"
        </th>
        <th>
            "ReceitaCategoriaID"
        </th>
        <th>
            "ReceitaAutorID"
        </th>
        <th>
            "Titulo"
        </th>
        <th></th>
    </tr>

    @foreach (var item in @Model)
    {
        <tr>
            @*<td>
                @Html.DisplayFor(modelItem => item.ReceitaID)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.ReceitaCategoriaID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReceitaAutorID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Titulo)
            </td>
            @*<td>
                @Html.ActionLink("Details", "Details", new { id = item.ReceitaID }) |
            </td>*@
        </tr>
    }
</table>
